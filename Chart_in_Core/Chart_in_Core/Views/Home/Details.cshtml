@using Microsoft.AspNetCore.Authorization
@using Satistic.Models.ViewModels
@model MoadiViewModel
<table class="table table-striped table-bordered zero-configuration dataTable" role="grid">
    <thead>
        <tr>
            <th>Name</th>
            <th>Brand</th>
            <th>Price</th>
            <th>ViewCount</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Lst_MoadiDetails != null)
            @foreach (var item in Model.Lst_MoadiDetails)
            {
                <tr>
                    <td>@item.Field_Name.ToString()</td>
                    <td>@item.Field_Value.ToString()</td>
                    <td></td>
                    <td></td>
                </tr>
            }
    </tbody>
</table>